{
  "expo": {
    "name": "Lucapp-multi",
    "slug": "Lucapp-multi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon_fan_made.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/icon_fan_made_foreground.png",
      "resizeMode": "contain",
      "backgroundColor": "#2B2B2B"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.Lucapp",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.Lucapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icon_fan_made_foreground.png",
        "backgroundColor": "#9f9f9f"
      }
    },
    "web": {
      "favicon": "./app/assets/icon_fan_made_foreground.png"
    }
  }
}
